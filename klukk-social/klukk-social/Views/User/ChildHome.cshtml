@using klukk_social.Models
@model klukk_social.Models.UserViewModel

@Html.Partial("PostStatusPartial", new Post(@Model.Person.Id, @Model.Person.FullName))

@foreach (var post in Model.Feed)
{
	UserViewModel commentPartial = new UserViewModel();
	commentPartial.Person = Model.Person;
	commentPartial.CurrentUserId = Model.CurrentUserId;
	commentPartial.Feed.Add(post);
    @Html.Partial("PostPartial", commentPartial)
}
