@using Microsoft.AspNet.Identity
@using klukk_social.Services
@{
	UserSerice us = new UserSerice();
	var ProfilePic = us.GetProfileURL(User.Identity.GetUserId());
}
<div class="sidebar">
	<img src="@ProfilePic" alt="Alternate Text" />
	<a href="#">Input Name</a>
	<input type="submit" value="Post SHits" class="btn btn-default btn-lg btn-block" />
	<input type="submit" value="Look at SHits" class="btn btn-default btn-lg btn-block" />
	<input type="submit" value="More SHits" class="btn btn-default btn-lg btn-block" />
	@Html.ActionLink("Settings", "ChildSettings", "User", new { @class = "btn btn-default btn-lg btn-block" })
</div>